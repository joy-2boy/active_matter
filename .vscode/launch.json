{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug My App",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/job",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            //"externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "job-build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
